    <style>
.btn-primary {
    color: white;
    background-color: #337ab7;
    border-color: #337ab7;
  }
  #top .login a.btn.btn-primary {
    color: white;
}
.btn-primary:hover {
    border-color: white !important;
}
.btn-primary:hover {
    color: #fff;
    background-color: #337ab7 !important;
}
#top .login a.btn.btn-primary:hover {
    color: #fff;
    border-color: white !important;
}
a {
    color: #337ab7;
    text-decoration: none;
}
a:hover, a:focus {
    color: #337ab7;
}
.panel.sidebar-menu .panel-heading h1, .panel.sidebar-menu .panel-heading h2, .panel.sidebar-menu .panel-heading h3, .panel.sidebar-menu .panel-heading h4, .panel.sidebar-menu .panel-heading h5 {
    display: inline-block;
    border-bottom: solid 5px #337ab7;
  }
.nav-pills>li.active>a, .nav-pills>li.active>a:hover, .nav-pills>li.active>a:focus {
    color: #ffffff;
    background-color: gray;
}
#footer ul a:hover {
    color: #337ab7;
    text-decoration: none;
}


.nav-pills>li.active>a, .nav-pills>li.active>a:hover, .nav-pills>li.active>a:focus {
    color: #ffffff;
    background-color: #337ab7;
}

.navbar ul.nav>li.active>a, .navbar ul.nav>li.open>a {
    text-decoration: none !important;
    border-top: solid 5px #294d76;
}

.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:hover, .navbar-default .navbar-nav>.active>a:focus {
    color: white;
    background-color: #467fbf;
}

button.btn.btn-no-margin {
    color: white;
    background-color: #337ab7;
    border: solid;
    border-width: 0.5px;
    height: 34px;
    border-color: #ccc;
}

button.btn.btn-no-margin:hover {
    color: white;
    background-color: #337ab7;
}

.nav-tabs {
    border-bottom: 1px solid #337ab7;
}

.navbar ul.nav>li>a:hover {
    border-top: solid 5px #467fbf;
}

.navbar-default .navbar-nav>li>a:hover, .navbar-default .navbar-nav>li>a:focus {
    color: #555555;
    background-color: #92b3d9;
}
.nav-tabs>li.active>a, .nav-tabs>li.active>a:hover, .nav-tabs>li.active>a:focus {
    color: #555555;
    background-color: #ffffff;
    border: 1px solid #337ab7;
    border-bottom-color: transparent;
    cursor: default;
}
.pagination>.active>a, .pagination>.active>span, .pagination>.active>a:hover, .pagination>.active>span:hover, .pagination>.active>a:focus, .pagination>.active>span:focus {
    z-index: 2;
    color: #ffffff;
    background-color: #337ab7;
    border-color: #337ab7;
}
.pagination>li>a, .pagination>li>span {
    padding: 6px 12px;
    line-height: 1.42857143;
    text-decoration: none;
    color: #337ab7;
    background-color: #ffffff;
    border: 1px solid #dddddd;
}
.heading h1, .heading h2, .heading h3, .heading h4, .heading h5 {
    border-bottom: solid 5px #337ab7;
}
.alert-success {
    background-color: #337ab7;
    border-color: #337ab7;
    color: white;
    margin-top: 2px;
}
.btn-primary:focus, .btn-primary.focus {
    color: white;
    background-color: #337ab7;
    border-color: #337ab7;
}

.product .text h3 {
    font-size: 14px;
    font-weight: 700;
    height: 40.6px;
    text-transform: uppercase;
    letter-spacing: 0.08em;
}

</style>
  </head>

  <body>
      <%= render 'layouts/top' %>
      <%= render 'layouts/navbar' %>
      <div id="all">
        <div id="content">
            <%= yield(:breadcrumbs) %>
            <div class="container">
              <%= render 'layouts/messages' %>
              <%= yield %>
            </div>
        </div>
        <%= render 'layouts/footer' %>
        <%= render 'layouts/copyright' %>
      </div>
    <%= javascript_include_tag 'application' %>
  </body>
</html>
